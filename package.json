{
  "name": "todo-api-server",
  "version": "1.0.0",
  "description": "A simple RESTful server for a react native todo app.",
  "main": "src/index.js",
  "scripts": {
    "prestart": "yarn docs",
    "start": "nodemon src/index.js --exec \"node --require 'dotenv-safe/config' --require 'babel-register'\"",
    "build": "babel src -d dist --require dotenv-safe/config",
    "serve": "node --require 'dotenv-safe/config' --require 'babel-register' dist/index.js",
    "docs": "apidoc -i src/ -o docs/"
  },
  "repository": "git@github.com:joeRob2468/todo-api-server.git",
  "author": "John Boyer",
  "license": "MIT",
  "apidoc": {
    "url": "localhost:3000/v1"
  },
  "private": true,
  "dependencies": {
    "apidoc": "^0.17.6",
    "babel-eslint": "^8.2.2",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv-safe": "^5.0.1",
    "eslint": "^4.18.2",
    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "http-status": "^1.0.1",
    "joi": "^13.1.2",
    "mongoose": "^5.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.17.1"
  }
}
